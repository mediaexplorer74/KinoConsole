<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Class="KinoConsole.BackDialog"
    mc:Ignorable="d"
    d:DesignHeight="800" 
    d:DesignWidth="480" 
    Opacity="0.85" >

    <StackPanel x:Name="LayoutRoot" Background="#FF2D282E" Margin="-60,292,-52,285" Width="600">
        <Button x:Name="btnKeyboard" Content="show keyboard" Foreground="#FFFFFFFF" BorderBrush="#FFFFFFFF"/>
        <Button x:Name="btnEdit" Content="edit controls" Foreground="#FFFFFFFF" BorderBrush="#FFFFFFFF"/>
        <Button x:Name="btnDisconnect" Content="disconnect" Foreground="#FFFFFFFF" BorderBrush="#FFFFFFFF"/>
    </StackPanel>
</UserControl>
